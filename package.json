  "scripts": {
    "dev": "vite --port 5080",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "npm run build && open dist/stats.html",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
